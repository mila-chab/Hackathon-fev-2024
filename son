import pygame
import time
import tkinter as tk
from tkinter import PhotoImage


def play_sound(file_path):
    pygame.mixer.init()
    pygame.mixer.music.load(file_path)
    pygame.mixer.music.play()

def stop_sound():
    pygame.mixer.music.stop()

play_sound("tetris.mp3") 
time.sleep(1000)
stop_sound()